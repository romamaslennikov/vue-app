{
  "name": "vue-app",
  "version": "1.0.0",
  "private": true,
  "developer": "telegram: @romamas",
  "scripts": {
    "dev": "vite",
    "build:prod": "vite build --mode prod",
    "build:stage": "vite build --mode stage",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "iconfont": "cd gulp && gulp iconfont",
    "spritePng": "cd gulp && gulp spritePng",
    "optimiz": "optimizt --webp --lossless src/assets/images/* && optimizt --avif src/assets/images/* && optimizt src/assets/images/*",
    "deploy": "node deploy",
    "husky-install": "npx husky-init && husky install && npx husky set .husky/pre-commit 'yarn lint'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^3.2.1",
