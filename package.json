    "click-outside-vue3": "^4.0.1",
    "js-cookie": "^3.0.5",
    "lodash.debounce": "^4.0.8",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.12.1",
    "typograf": "^7.4.1",
    "vue": "^3.*",
    "vue-router": "^4.3.2",
    "vue3-touch-events": "^4.1.8"
